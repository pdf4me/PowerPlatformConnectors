{
  "properties": {
    "connectionParameters": {
      "token": {
        "type": "oauthSetting",
        "oAuthSettings": {
          "identityProvider": "oauth2generic",
          "properties": {
            "ItestfirstParty": "true",
            "IsOnbehalfofLoginSupported": false
          },
          "customParameters": {
            "tokenUrlTemplate": {
              "value": "https://apiv2.eightfold-wu.ai/oauth/v1/authenticate"
            },
            "tokenBodyTemplate": {
              "value": "grant_type=password&username={userName}&password={password}"
            }
          }
        }
      }
    },
    "iconBrandColor": "#008000",
    "publisher": "Microsoft"
  }
}